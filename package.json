{
  "name": "eslint-plugin-react-jsx-props-spread-first",
  "version": "1.0.0",
  "description": "This ESLint plugin enforces that spread operators in React JSX props appear at the beginning of the props list.",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "lint": "run-s lint:ts lint:eslint lint:prettier",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint '**/*.{js,ts}'",
    "lint:prettier": "prettier '**/*.{js,ts,json,yaml,md}'",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "release": "semantic-release",
    "add-rule": "ts-node scripts/add-rule",
    "update": "ts-node scripts/update"
  },
  "peerDependencies": {
    "eslint": ">=5.16.0"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.62.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.8.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "lint --fix",
      "prettier --write"
    ],
    "*.{json,yaml,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huuyafwww/eslint-plugin-react-jsx-props-spread-first"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "react-props",
    "jsx-props",
    "spread-first"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huuyafwww/eslint-plugin-react-jsx-props-spread-first/issues"
  },
  "homepage": "https://github.com/huuyafwww/eslint-plugin-react-jsx-props-spread-first#readme"
}
